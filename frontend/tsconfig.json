{
    "compilerOptions": {
        "jsx": "react",
        "module": "esnext",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "plugins": [
            { "transform": "./node_modules/jsx-conditionals/transform.js" },
        ]
    }
}